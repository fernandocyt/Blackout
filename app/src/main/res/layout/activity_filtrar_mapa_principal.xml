<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="losmarinos.blackout.Actividades.FiltrarMapaPrincipal">


    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:text="Filtrar"
            android:textSize="25dp" />

        <Switch
            android:id="@+id/switch_cortes_filtrar_mapa_principal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:text="Cortes"
            android:textSize="20dp" />


        <Switch
            android:id="@+id/switch_reportes_filtrar_mapa_principal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:text="Reportes"
            android:textSize="20dp" />


        <Switch
            android:id="@+id/switch_sucursales_filtrar_mapa_principal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:text="Sucursales"
            android:textSize="20dp" />


    </LinearLayout>
</android.support.constraint.ConstraintLayout>
